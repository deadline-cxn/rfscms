	background-color: #880000;
}

.contenttd {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: small;
    font-weight: bold;
	padding: 5px;
	color:   #FFFFFF;	
	background-color:rgba(0,0,0,0.6);
}

.toptd {
    font-family: Verdana;
    font-size: 1px;
    font-style: italic;
    font-weight: bold;
    color:   #000000;
	background-color:rgba(0,0,0,0.4);
	border-bottom: 1px solid #000;
}

.logged_in_td {
	background-color:rgba(0,0,0,0.4);
	border-bottom: 1px solid #000;
}

.lefttd {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: xx-small;
    font-weight: bold;
    color:   #00FF00; 
	background-color:rgba(0,0,0,0.2);
	width: 200px;
	min-width: 200px;
	max-width: 200px;
}

.midtd{
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: small;
	font-weight: bold;
	border: 1px solid #000;
	background-color:rgba(0,0,0,0.7);
	
	
}

.righttd {
	background-color:rgba(0,5,0,0.8);
	width: 200px;
