.forum_box {
	width: 100%;
	color: #00FF00;
	background-color: #114411;
	float: left;
	overflow: hidden;
}

.forum_user{
	float: left;
	min-height: 150px;
	background-color: #225522;
	border: 1px dashed #00ff00;
	border-radius: 8px;
	padding: 5px;
}

.forum_message{
	overflow: hidden;
	min-height: 160px;	
	color: #00FF00;
	// ackground-color: #000000;
	padding: 5px;
	
}

.forum_time{
	overflow: hidden;
	clear: both;
	color: #112211;	
	background-color: #225522;
	
}


.rfs_forum_table_header {
	background-color: #2c5c2c;
}

.rfs_forum_table_1{
	background-color: #407c40;
	color: #000000;
}

.rfs_forum_table_2 {
	background-color: #3c6c3c;
	color: #000000;
}


.file_box{

	padding: 20px;
}

.file_list { 

	width: 100%;
	
}

.file_category_icon {
	width: 64px;
	height: 64px;
	border: 0;
	position: relative;
	float: left;
	padding: 10px;
	display: inline;
}

.file_category {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    color:  #0f0;
    background-color:rgba(50,250,50,0.4);
	width: 99%;
	padding-top: 20px;
	padding-left: 10px;
	font-style: none;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;
	margin-top: 20px;
	position: relative;
	float: left;
	display: inline-flex;
	flex-direction: row;
	#align-items: center;
	#margin: 0 2px 5px;
	#word-break: break-word;
	
}

.file_category_link, .file_category_link:link {
	vertical-align: top;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 25px;
	font-style: none;
	text-decoration: none;
	color: #0f0;
	padding-top:5px;
	padding-left: 20px;
	
}

.file_link, .file_link:link {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: none;
	color: #0f0;
}

.rfs_file_icon {
	width: 32px;
	height: 32px;
	padding: 5px; 
	float: left;
}

.rfs_file_table_outer_0 {
	background-color:rgba(0,260,0,0.1);
	border-top: 1px dashed #070; 
	padding: 40px;
}
.rfs_file_table_0 {
	color: #00FF00;
	width: 100%;
	clear: both;	
}
.rfs_file_table_inner_0 {
	position: relative;
	float: left;
	#display: inline-flex;
	#flex-direction: column;
	padding: 10px;
	
}

.rfs_file_table_outer_1 {
	background-color:rgba(0,160,0,0.1);
	border-top: 1px dashed #070; 
	padding: 40px;
}
.rfs_file_table_1 {	
 	color: #00FF00;
	width: 100%;
	clear: both;
}
.rfs_file_table_inner_1 {
	position: relative;
	float: left;
