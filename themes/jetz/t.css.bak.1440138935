body{
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: small;
    font-style: normal;
	color: #FFF;
	background-image: url('/themes/default/t.background.jpg');
	background-repeat:no-repeat;
	background-attachment:fixed;
	margin:0px;

}


.tags {
	padding: 2px;
	margin: 2px;
	border: 1px solid #2f2;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
	color: #232;
	background-color: #4a4;
	#float: left;
	display: inline-table;
	
}



.news_box {
	clear: both;
	margin: 5px;
	padding: 5px;
}

.news_headline_bar {
	background-color:rgba(0,200,0,0.1);
	height: 25px;
	padding: 5px;
	border-bottom: 1px solid #050;
	tborder-top-left-radius: 15px;
	rtborder-top-right-radius: 15px;
}
.news_headline {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: large;
    font-weight: bold;
    font-style: italic;
    color: #070;
	float: left;
	padding: 5px;
}

.news_time {
	font-style: italic;
	padding: 5px 25px;
	vertical-align: text-bottom;
	float: left;
}

.news_image {
	
	float: left;
	padding: 5px 20px 15px 10px;
	
}

.news_article {
	border-top: 1px solid #000;
	clear: both; 
	padding: 25px;
	background-color:rgba(0,0,0,0.1);
	margin 0 0 25px 0;
}

.news_edit_bar {
	border-top: 1px solid #000;
	clear: both;
	background-color:rgba(0,160,150,0.1);
	padding: 25px;
	margin: 0 0 25px 0;
	border-bottom-left-radius: 15px;
	border-bottom-right-radius: 15px;
}

.wikishell {
	margin:0 auto;
	padding: 10px;
	align: center;
	width: 90%;
	font-family: monospace;
	background-color: #050;
color: #00ffff;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;
	border-bottom-left-radius: 8px;
	border-bottom-right-radius: 8px;
}


.rfs_thumb {
	background-color: #282;
	box-shadow: 3px 3px 3px #228822;
	border:solid 1px #228822;
	border-radius:5px;
}

.slogan {
    font-family: Verdana;
    font-size: small;
    font-style: italic ;
color: #00ffff;
}

.warning {
  font-family: Lucidia;
  font-size: x-large;
  font-style: normal;
  color: #FFFFFF;
background-color: #0080c0;
}


.rfs_top_menu_table{
  font-family: Lucidia; 
  font-style: normal;  
}

.rfs_top_menu_link {
    font-family: Lucidia;
    font-size: 23px;
    font-weight: bold;
    font-style: normal;
    text-decoration: none;
color: #00ffff;
	padding-right: 30px;
}
.rfs_top_menu_link:link {
color: #00ffff;
	text-shadow: 2px 2px 2px #000;

}
.rfs_top_menu_link:hover {
	background-color:  #007700;
color: #00ffff;
}

hr {
	border: 1px dashed #0f0;
}

h1 {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 20px;;
    font-weight: bold;
    font-style: italic;
    
color: #00ffff;
	background-color:rgba(0,0,0,0.2);
	padding:0;
	margin:0;
}
h2 {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 18px;;
    font-weight: bold;
    font-style: italic;
    
	color: #009900;
	
	background-color:rgba(0,0,0,0.2);
	padding:0;
	margin:0;

}
h3 {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 16px;;
    font-weight: bold;
    font-style: italic;
color: #00ffff;

}

.font {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: small;
    font-style: normal;
    color: #FFFFFF;
}

td {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: small;
    font-weight: bold;
    color:  #009900;
}

.downloadtd {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 12px;
    color:   #039700;
	background-color: #002200;
}
